@model IEnumerable<Vendor.Domain.Entites.Branch.BranchStaff>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TechnicalCount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NonTechnicalCount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UploadInsurance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BranchStaffType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TechnicalCount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NonTechnicalCount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UploadInsurance)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BranchStaffType.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
